<?xml version="1.0" encoding="UTF-8"?>
<sign-service-config-global xsi:schemaLocation="http://dom.gosuslugi.ru/schema/signature/config/global sign-service-config-global.xsd"
                            xmlns="http://dom.gosuslugi.ru/schema/signature/config/global" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <rootCertificates>
        <rootCertificate id="guc2012">
            <base64Encoded></base64Encoded>
        </rootCertificate>
        <rootCertificate id="guc2001">
            <base64Encoded></base64Encoded>
        </rootCertificate>
    </rootCertificates>
    <downloaderSettings>
        <downloadThreadCount>10</downloadThreadCount>
        <defaultMaxPerRoute>2</defaultMaxPerRoute>
        <socketTimeout>0</socketTimeout>
        <connectionTimeout>0</connectionTimeout>
    </downloaderSettings>
    <crlSettings>
        <downloadTimeout>60000</downloadTimeout>
        <updatePeriod>60</updatePeriod>
        <cleanupPeriod>60</cleanupPeriod>
        <fastRemoval>false</fastRemoval>
    </crlSettings>
    <tslSettings>
        <tslUrl>http://e-trust.gosuslugi.ru/CA/DownloadTSL?schemaVersion=0</tslUrl>
        <downloadTimeout>450000</downloadTimeout>
        <updatePeriod>720</updatePeriod>
    </tslSettings>
    <authSettings>
        <caller enabled="true">
            <name>Демо-приложение</name>
            <apiKey>demo_secret</apiKey>
            <sign enabled="true">
                <keyId>ID_1</keyId>
            </sign>
            <verify enabled="true"/>
            <digest enabled="true"/>
            <management enabled="true"/>
            <monitoring enabled="true"/>
        </caller>
        <anonymous>
            <verify enabled="false"/>
            <digest enabled="false"/>
            <management enabled="false"/>
            <monitoring enabled="true"/>
        </anonymous>
    </authSettings>
    <skipCAConformityCheck>true</skipCAConformityCheck>
    <includeStacktrace>false</includeStacktrace>
</sign-service-config-global>
